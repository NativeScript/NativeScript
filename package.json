    "shady-css-parser": "^0.1.0",
    "terser-webpack-plugin": "~3.0.6",
    "tree-kill": "^1.2.2",
    "ts-jest": "27.0.5",
    "ts-node": "9.1.1",
    "ts-patch": "^1.3.0",
    "tslint": "6.1.3",
    "typedoc": "^0.20.14",
    "typescript": "4.3.5",
    "webpack": "~4.44.1",
    "webpack-cli": "~3.3.12",
    "zx": "^4.2.0"
  },
  "lint-staged": {
    "**/*": [
      "nx format:write --files"
