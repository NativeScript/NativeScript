		"acorn": "^8.14.0",
		"acorn-stage3": "^4.0.0",
		"ansi-colors": "^4.1.3",
		"babel-loader": "^8.0.0",
		"cli-highlight": "^2.1.11",
		"commander": "^8.3.0",
		"css-loader": "^7.1.2",
		"dotenv-webpack": "^8.1.0",
		"loader-utils": "^3.3.1",
		"postcss": "^8.5.3",
		"postcss-import": "^16.1.0",
		"postcss-loader": "^8.1.1",
		"raw-loader": "^4.0.2",
		"react-refresh": "^0.14.2",
		"rspack-plugin-virtual-module": "^0.1.13",
		"sass": "^1.86.0",
		"sass-embedded": "^1.86.0",
		"sass-loader": "^16.0.5",
