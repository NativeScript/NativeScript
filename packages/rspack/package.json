		"ts-dedent": "^2.2.0",
		"vue-loader": "^17.0.0",
		"webpack-bundle-analyzer": "^4.10.2",
		"webpack-merge": "^5.10.0",
		"rspack-chain": "^1.2.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.0.0",
		"@angular/compiler-cli": "^19.0.0",
		"@swc/helpers": "^0.5.15",
		"@types/css": "0.0.33",
		"@types/jest": "29.5.4",
		"@types/loader-utils": "2.0.3",
		"@types/lodash.get": "4.4.7",
