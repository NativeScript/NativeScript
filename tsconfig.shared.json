{
    "compilerOptions": {
        "noEmitOnError": false,
        "noEmitHelpers": true,
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "noImplicitUseStrict": true,
        "removeComments": true,
        "experimentalDecorators": true,
        "diagnostics": true,
        "sourceMap": true,
        "jsx": "react",
        "reactNamespace": "UIBuilder",
        "lib": [
            "es6", "dom"
        ],
        "baseUrl": ".",
        "paths": {
            "*": [
                "./tns-core-modules/*",
                "./node_modules/*"
            ]
        }
    }
}
